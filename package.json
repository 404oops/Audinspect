{
  "name": "audinspect",
  "version": "2.1.0",
  "description": "",
  "main": "electron/main.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "build": "vite build",
    "package": "npm run build && electron-builder && node scripts/clean-build.js",
    "package:win_x64": "npm run build && electron-builder --win --x64 && node scripts/clean-build.js",
    "package:win_arm": "npm run build && electron-builder --win --arm64 && node scripts/clean-build.js",
    "package:mac_x64": "npm run build && electron-builder --mac --x64 && node scripts/clean-build.js",
    "package:mac_arm": "npm run build && electron-builder --mac --arm64 && node scripts/clean-build.js",
    "package:linux": "npm run build && electron-builder --linux --x64 && node scripts/clean-build.js",
    "package:all": "npm run build && electron-builder -mwl && node scripts/clean-build.js",
    "postinstall": "electron-rebuild && electron-builder install-app-deps"
  },
  "keywords": [],
  "author": "",
  "license": "GPL-3.0-only",
  "type": "commonjs",
  "build": {
    "appId": "com.404oops.audinspect",
    "icon": "resources/icon.png",
    "publish": null,
    "asar": true,
    "files": [
      "dist/*.html",
      "dist/assets/*",
      "electron/*",
      "package.json",
      "!**/node_modules/**/*.md",
      "!**/node_modules/**/*.d.ts",
      "!**/node_modules/**/*.d.cts",
      "!**/node_modules/**/*.d.mts",
      "!**/node_modules/**/*.map",
      "!**/node_modules/**/*.ts",
      "!**/node_modules/**/*.tsx",
      "!**/node_modules/**/*.coffee",
      "!**/node_modules/**/*.flow",
      "!**/node_modules/**/test/**",
      "!**/node_modules/**/tests/**",
      "!**/node_modules/**/__tests__/**",
      "!**/node_modules/**/docs/**",
      "!**/node_modules/**/doc/**",
      "!**/node_modules/**/documentation/**",
      "!**/node_modules/**/example/**",
      "!**/node_modules/**/examples/**",
      "!**/node_modules/**/.github/**",
      "!**/node_modules/**/.vscode/**",
      "!**/node_modules/**/.idea/**",
      "!**/node_modules/**/CHANGELOG*",
      "!**/node_modules/**/HISTORY*",
      "!**/node_modules/**/CHANGES*",
      "!**/node_modules/**/AUTHORS*",
      "!**/node_modules/**/CONTRIBUTORS*",
      "!**/node_modules/**/*.gyp",
      "!**/node_modules/**/*.gypi",
      "!**/node_modules/**/binding.gyp",
      "!**/node_modules/**/.npmignore",
      "!**/node_modules/**/.eslintrc*",
      "!**/node_modules/**/.prettierrc*",
      "!**/node_modules/**/.editorconfig",
      "!**/node_modules/**/.travis.yml",
      "!**/node_modules/**/.babelrc*",
      "!**/node_modules/**/tsconfig*.json",
      "!**/node_modules/**/jest.config.*",
      "!**/node_modules/**/karma.conf.*",
      "!**/node_modules/**/webpack.config.*",
      "!**/node_modules/**/rollup.config.*",
      "!**/node_modules/**/Makefile",
      "!**/node_modules/**/Gruntfile*",
      "!**/node_modules/**/gulpfile*",
      "!**/node_modules/**/*.min.js.map",
      "!**/node_modules/**/*.mjs.map",
      "!**/node_modules/**/*.cjs.map",
      "!**/node_modules/**/src/**",
      "!**/node_modules/**/source/**",
      "!**/node_modules/**/coverage/**",
      "!**/node_modules/**/.nyc_output/**",
      "!**/node_modules/**/benchmark/**",
      "!**/node_modules/**/benchmarks/**",
      "!**/node_modules/**/fixture/**",
      "!**/node_modules/**/fixtures/**",
      "!**/node_modules/**/__mocks__/**",
      "!**/node_modules/**/__fixtures__/**",
      "!**/node_modules/ffmpeg-static/**",
      "!**/node_modules/ffprobe-static/**"
    ],
    "directories": {
      "output": "build"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": "x64"
        }
      ],
      "files": [
        "!**/prebuilds/darwin-*/**",
        "!**/prebuilds/linux-*/**",
        "!**/prebuilds/*-arm/**",
        "!**/prebuilds/*-arm64/**",
        "!**/prebuilds/*-ia32/**"
      ]
    },
    "nsis": {
      "oneClick": true,
      "perMachine": false,
      "deleteAppDataOnUninstall": false
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": "arm64"
        }
      ],
      "files": [
        "!**/prebuilds/win32-*/**",
        "!**/prebuilds/linux-*/**",
        "!**/prebuilds/*-ia32/**"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "files": [
        "!**/prebuilds/win32-*/**",
        "!**/prebuilds/darwin-*/**",
        "!**/prebuilds/*-ia32/**"
      ]
    },
    "compression": "maximum"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.7.1",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "electron": "^39.2.3",
    "electron-builder": "^25.1.8",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "upx": "^1.0.6",
    "vite": "^7.2.4",
    "wait-on": "^9.0.3"
  },
  "optionalDependencies": {
    "ffmpeg-static": "^5.3.0",
    "ffprobe-static": "^3.1.0"
  },
  "dependencies": {
    "chokidar": "^5.0.0",
    "classic-level": "^3.0.0",
    "level": "^10.0.0",
    "level-supports": "^6.0.0",
    "level-transcoder": "^1.0.1",
    "dexie": "^4.2.1",
    "lucide-react": "^0.554.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-window": "^2.2.3",
    "wavesurfer.js": "^7.0.0",
    "zustand": "^5.0.8"
  }
}
